# API Keys Configuration
# Copy this file to .env and fill in your actual API keys

# Google Gemini API Key
GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# Together AI API Key
TOGETHER_AI_API_KEY=YOUR_TOGETHERAI_KEY

# SerpAPI Key for plagiarism checking
SERP_API_KEY=YOUR_SERPAPI_KEY

# OpenAlex API Key (Optional for academic search)
OPENALEX_API_KEY=YOUR_OPENALEX_API_KEY

# Crossref API Key (Optional for citation data)
CROSSREF_API_KEY=YOUR_CROSSREF_API_KEY

# Server Port (optional, defaults to 3000)
PORT=3000
